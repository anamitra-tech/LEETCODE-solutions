int findContentChildren(vector<int>& g, vector<int>& s) {
    sort(g.begin(), g.end()); 
    sort(s.begin(), s.end()); 

    int i = 0; 
    int j = 0; 
    int n = g.size(), m = s.size();
    int count = 0;

    while (i < n && j < m) {
        if (s[j] >= g[i]) { 
            count++; 
            i++; j++;       // move both pointers
        } else {
            j++; // cookie too small 
        }
    }

    return count;
}
