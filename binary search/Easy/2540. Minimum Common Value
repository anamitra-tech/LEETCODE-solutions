
    int minimumCommonValue(vector<int>& nums1, vector<int>& nums2) {
        for (int x : nums1) {
            int s = 0, e = nums2.size() - 1;
            while (s <= e) {
                int mid = s + (e - s) / 2;
                if (nums2[mid] == x) return x; // found common
                else if (nums2[mid] < x) s = mid + 1;
                else e = mid - 1;
            }
        }
        return -1; // no common element
    }
