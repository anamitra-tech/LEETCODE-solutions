int maximumCount(vector<int>& nums) {
    int n = nums.size();
    int s = 0, e = n - 1;

    while (s <= e) {
        int mid = s + (e - s) / 2;
        if (nums[mid] < 0) s = mid + 1;
        else e = mid - 1;
    }

    int firstPositive = s;      
    int positiveCount = n - firstPositive;
    int negativecount= s;


    return max(negativeCount, positiveCount);
}


